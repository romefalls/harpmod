            ImageLabel.Size = UDim2.new(0, 15, 0, 15)
            ImageLabel.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"

            TextBox_2.Focused:Connect(function()
                ts:Create(TextBox_2,TweenInfo.new(.4),{TextStrokeTransparency = 0,TextTransparency = 0}):Play()
            end)

            TextBox_2.FocusLost:Connect(function()
                f(TextBox_2.Text)
                ts:Create(TextBox_2,TweenInfo.new(.4),{TextStrokeTransparency = 1,TextTransparency = 1}):Play()
            end)
        end

        function self2:Slider(name,min,max,default,precise,f)
            local Slider = Instance.new("Frame")
            local UICorner = Instance.new("UICorner")
            local Slid = Instance.new("TextButton")
            local TextButton = Instance.new("TextButton")
            local UICorner_2 = Instance.new("UICorner")
            local UICorner_3 = Instance.new("UICorner")
            local _2 = Instance.new("TextBox")
            local maketextlabel = Instance.new("TextLabel")
            local ImageLabel = Instance.new("ImageLabel")
            local con

            Slider.Name = "Slider"
            Slider.Parent = Tab
            Slider.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
            Slider.BorderSizePixel = 0
            Slider.Position = UDim2.new(0, 0, 0.196911201, 0)
            Slider.Size = UDim2.new(0, 361, 0, 15)

            UICorner.CornerRadius = UDim.new(0, 3)
            UICorner.Parent = Slider

            Slid.Name = "Slid"
            Slid.Parent = Slider
            Slid.BackgroundColor3 = Color3.fromRGB(11, 11, 11)
            Slid.Position = UDim2.new(0.501483679, 0, 0.333333343, 0)
            Slid.Size = UDim2.new(0, 168, 0, 4)
            Slid.AutoButtonColor = false
            Slid.Font = Enum.Font.SourceSans
            Slid.Text = ""
            Slid.TextColor3 = Color3.fromRGB(0, 0, 0)
            Slid.TextSize = 14.000

            TextButton.Parent = Slid
            TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextButton.Position = UDim2.new(0.244047627, 0, -0.785714269, 0)
            TextButton.Size = UDim2.new(0, 11, 0, 11)
            TextButton.AutoButtonColor = false
            TextButton.Font = Enum.Font.SourceSans
            TextButton.Text = ""
            TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
            TextButton.TextSize = 14.000

            UICorner_2.CornerRadius = UDim.new(0, 150)
            UICorner_2.Parent = TextButton

            UICorner_3.CornerRadius = UDim.new(0, 3)
            UICorner_3.Parent = Slid

            _2.Name = "2"
            _2.Parent = Slider
            _2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            _2.BackgroundTransparency = 1.000
            _2.Position = UDim2.new(0.356083095, 0, 0, 0)
            _2.Size = UDim2.new(0, 48, 0, 15)
            _2.Font = Enum.Font.GothamSemibold
            _2.Text = ""
            _2.TextColor3 = Color3.fromRGB(255, 255, 255)
            _2.TextSize = 14.000

            maketextlabel.Name = "maketextlabel"
            maketextlabel.Parent = Slider
            maketextlabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            maketextlabel.BackgroundTransparency = 1.000
            maketextlabel.Position = UDim2.new(0.0741839781, 0, 0, 0)
            maketextlabel.Size = UDim2.new(0, 93, 0, 15)
            maketextlabel.Font = Enum.Font.GothamSemibold
            maketextlabel.Text = name
            maketextlabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            maketextlabel.TextSize = 14.000
            maketextlabel.TextStrokeTransparency = 0.700
            maketextlabel.TextXAlignment = Enum.TextXAlignment.Left

            ImageLabel.Parent = Slider
            ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ImageLabel.BackgroundTransparency = 1.000
            ImageLabel.BorderSizePixel = 0
            ImageLabel.Position = UDim2.new(0, 6, 0, 0)
            ImageLabel.Size = UDim2.new(0, 15, 0, 15)
            ImageLabel.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"

            local function move()
                if not con then
        			con = game:GetService("RunService").Stepped:Connect(function()
        				local m = game:GetService("UserInputService"):GetMouseLocation()
        				local r = math.clamp(((m.X-TextButton.AbsoluteSize.X) - Slid.AbsolutePosition.X)/(Slid.AbsoluteSize.X),0,1)
        				local vtn = min + (max - min)*r
        
        				vtn = math.clamp(vtn,min,max)
                        ts:Create(TextButton,TweenInfo.new(.05),{Position = UDim2.new(r, 0, -0.785714269,0)}):Play()
        
        				if not precise then
							vtn = math.round(vtn)
						else
							vtn = tonumber(tostring(vtn):sub(1,4))
						end

						_2.Text = tostring(vtn).."/"..tostring(max)
						f(vtn)
        			end)
                end
            end

            _2.FocusLost:Connect(function()
                local v = tonumber(_2.Text:split("/")[1])
                local r = 1 - ((max - v)/(max - min))
    			local num = v
    
    			if not precise then
                    num = math.round(num)
                else
                    num = tonumber(tostring(num):sub(1,4))
                end

                ts:Create(TextButton,TweenInfo.new(.05),{Position = UDim2.new(r, 0, 0, 1)}):Play()
    
                f(num)
    			_2.Text = tostring(num).."/"..tostring(max)
            end)

            game:GetService("UserInputService").InputEnded:Connect(function(g)
                if g.UserInputType == Enum.UserInputType.MouseButton1 and con ~= nil and con.Connected then
                    con:Disconnect()
                end
            end)

            Slid.MouseButton1Down:Connect(move)
            TextButton.MouseButton1Down:Connect(move)
        end

        function self2:Dropdown(n,tbl,f)
            local Dropdown = Instance.new("Frame")
            local _1 = Instance.new("TextButton")
            local UICorner = Instance.new("UICorner")
            local ImageButton = Instance.new("ImageButton")
            local DFrame = Instance.new("Frame")
            local UIGridLayout = Instance.new("UIGridLayout")
            local UICorner_2 = Instance.new("UICorner")
            local UIPadding = Instance.new("UIPadding")
            local ImageLabel = Instance.new("ImageLabel")
            local open = false

            Dropdown.Name = "Dropdown"
            Dropdown.Parent = Tab
            Dropdown.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
            Dropdown.BorderSizePixel = 0
            Dropdown.Position = UDim2.new(0, 0, 0.262548268, 0)
            Dropdown.Size = UDim2.new(0, 361, 0, 15)
            Dropdown.ZIndex = 3

            _1.Name = "1"
            _1.Parent = Dropdown
            _1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            _1.BackgroundTransparency = 1.000
            _1.Position = UDim2.new(0.0741840079, 0, 0, 0)
            _1.Size = UDim2.new(0, 306, 0, 15)
            _1.Font = Enum.Font.GothamSemibold
            _1.Text = n
            _1.TextColor3 = Color3.fromRGB(255, 255, 255)
            _1.TextSize = 14.000
            _1.TextStrokeTransparency = 0.700
            _1.TextXAlignment = Enum.TextXAlignment.Left
            _1.ZIndex = 3

            UICorner.CornerRadius = UDim.new(0, 3)
            UICorner.Parent = Dropdown

            ImageButton.Parent = Dropdown
            ImageButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ImageButton.BackgroundTransparency = 1.000
            ImageButton.Position = UDim2.new(0.923054159, 0, 0, 0)
            ImageButton.Size = UDim2.new(0, 15, 0, 15)
            ImageButton.Image = "rbxassetid://6545531971"
            ImageButton.ZIndex = 3

            DFrame.Name = "DFrame"
            DFrame.Parent = Dropdown
            DFrame.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
            DFrame.Position = UDim2.new(0, 0, 0.919999182, 0)
            DFrame.Size = UDim2.new(0, 361, 0, 67)
            DFrame.Visible = false
            DFrame.ZIndex = 3

            UIGridLayout.Parent = DFrame
            UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
            UIGridLayout.CellPadding = UDim2.new(0, 5, 0, 2)
            UIGridLayout.CellSize = UDim2.new(0, 320, 0, 15)

            UICorner_2.CornerRadius = UDim.new(0, 3)
            UICorner_2.Parent = DFrame

            UIPadding.Parent = DFrame
            UIPadding.PaddingBottom = UDim.new(0, 5)
            UIPadding.PaddingLeft = UDim.new(0, 24)
            UIPadding.PaddingTop = UDim.new(0, 10)

            ImageLabel.Parent = Dropdown
            ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ImageLabel.BackgroundTransparency = 1.000
            ImageLabel.BorderSizePixel = 0
            ImageLabel.Position = UDim2.new(0, 6, 0, 0)
            ImageLabel.Size = UDim2.new(0, 15, 0, 15)
            ImageLabel.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
            ImageLabel.ZIndex = 3

            local function drop()
                local b = 35
                for i = 1,#tbl do
                    b = b + 15
                end

                if open then
                    open = false
                    table.foreach(DFrame:GetChildren(),function(i,v)
                        if v:IsA("TextButton") then
                            ts:Create(v,TweenInfo.new(.4),{TextStrokeTransparency = 1, TextTransparency = 1}):Play()
                            task.delay(.4,function()
                                v.Visible = false
                            end)
                        end
                    end)
                    task.wait(.4)
                    ts:Create(DFrame,TweenInfo.new(.4),{Size = UDim2.new(0, 361, 0, 0)}):Play()
                    task.delay(.4,function()
                        DFrame.Visible = false
                    end)
                else
                    open = true
                    DFrame.Visible = true
                    ts:Create(DFrame,TweenInfo.new(.4),{Size = UDim2.new(0, 361, 0, b)}):Play()
                    task.delay(.4,function()
                        table.foreach(DFrame:GetChildren(),function(i,v)
                            if v:IsA("TextButton") then
                                ts:Create(v,TweenInfo.new(.4),{TextStrokeTransparency = .7, TextTransparency = 0}):Play()
                                v.Visible = true
                                v.TextColor3 = Color3.new(1,1,1)
                            end
                        end)
                    end)
                end
            end

            local function add(n)
                local Option = Instance.new("TextButton")

                Option.Name = "Option"
                Option.Parent = DFrame
                Option.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Option.BackgroundTransparency = 1.000
                Option.Size = UDim2.new(0, 325, 0, 15)
                Option.Font = Enum.Font.GothamSemibold
                Option.Text = n
                Option.TextColor3 = Color3.fromRGB(255, 255, 255)
                Option.TextSize = 15.000
                Option.TextXAlignment = Enum.TextXAlignment.Left
                Option.ZIndex = 3

                Option.Activated:Connect(function()
                    ts:Create(Option,TweenInfo.new(.2),{TextColor3 = Color3.fromRGB(171,171,171)}):Play()
                    task.delay(.4,drop)
                end)
            end

            _1.Activated:Connect(drop)
            ImageButton.Activated:Connect(drop)

            for i,v in pairs(tbl) do
                add(v)
            end
        end

        function self2:Keybind(name,default,f)
            local KeyBind = Instance.new("Frame")
