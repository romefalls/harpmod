    ImageLabel.Size = UDim2.new(0, 23, 0, 23)
    ImageLabel.Image = "rbxassetid://9349211660"
    ImageLabel.ImageColor3 = Color3.fromRGB(171, 171, 171)

    TabFrames.Name = "TabFrames"
    TabFrames.Parent = Main

    function self:Tab(Name)
        local self2 = {}
        
        local Tab = Instance.new("ScrollingFrame")
        local UIPadding = Instance.new("UIPadding")
        local UIListLayout = Instance.new("UIListLayout")
        local TextButton = Instance.new("TextButton")
        local UICorner = Instance.new("UICorner")

        TextButton.Parent = Tabs
        TextButton.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
        TextButton.Size = UDim2.new(0, 86, 0, 20)
        TextButton.Font = Enum.Font.Gotham
        TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TextButton.TextSize = 12.000
        TextButton.TextStrokeTransparency = 0.900
        TextButton.TextWrapped = true

        UICorner.CornerRadius = UDim.new(0, 3)
        UICorner.Parent = TextButton

        Tab.Name = "Tab"
        Tab.Parent = TabFrames
        Tab.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
        Tab.BorderSizePixel = 0
        Tab.Position = UDim2.new(0.209215134, 0, 0.101307191, 0)
        Tab.Size = UDim2.new(0, 372, 0, 269)
        Tab.AutomaticCanvasSize = Enum.AutomaticSize.Y
        Tab.ScrollBarThickness = 0

