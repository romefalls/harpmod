            _1.Activated:Connect(f)
        end

        function self2:Toggle(name,defualt,f)
            local Toggle = Instance.new("Frame")
            local _1 = Instance.new("TextButton")
            local ToggleParent = Instance.new("TextButton")
            local Toggle_2 = Instance.new("TextButton")
            local UICorner = Instance.new("UICorner")
            local UICorner_2 = Instance.new("UICorner")
            local UICorner_3 = Instance.new("UICorner")
            local ImageLabel = Instance.new("ImageLabel")
            local on = defualt
            local onp = UDim2.new(0.625, 0, 0, 0)
            local onc = Color3.new(1,1,1)
            local offp = UDim2.new(0,0,0,0)
            local offc = Color3.fromRGB(117, 117, 117)

            Toggle.Name = "Toggle"
            Toggle.Parent = Tab
            Toggle.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
            Toggle.BorderSizePixel = 0
            Toggle.Position = UDim2.new(0, 0, 0.131274134, 0)
            Toggle.Size = UDim2.new(0, 361, 0, 15)

            _1.Name = "1"
            _1.Parent = Toggle
            _1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            _1.BackgroundTransparency = 1.000
            _1.Position = UDim2.new(0.0741839781, 0, 0, 0)
            _1.Size = UDim2.new(0, 143, 0, 15)
            _1.Font = Enum.Font.GothamSemibold
            _1.Text = name
            _1.TextColor3 = Color3.fromRGB(255, 255, 255)
            _1.TextSize = 14.000
            _1.TextStrokeTransparency = 0.700
            _1.TextXAlignment = Enum.TextXAlignment.Left

            ToggleParent.Name = "ToggleParent"
            ToggleParent.Parent = Toggle
            ToggleParent.BackgroundColor3 = Color3.fromRGB(11, 11, 11)
            ToggleParent.Position = UDim2.new(0.504451036, 0, 0, 0)
            ToggleParent.Size = UDim2.new(0, 40, 0, 15)
            ToggleParent.Text = ""
            ToggleParent.AutoButtonColor = false

            Toggle_2.Name = "Toggle"
            Toggle_2.Parent = ToggleParent
            Toggle_2.Text = ""
            Toggle_2.AutoButtonColor = false
            Toggle_2.BackgroundColor3 = ((on and onc) or (not on and offc))
            Toggle_2.Size = UDim2.new(0, 15, 0, 15)
            Toggle_2.Position = ((on and onp) or (not on and offp))

            UICorner.CornerRadius = UDim.new(0, 150)
            UICorner.Parent = Toggle_2

            UICorner_2.CornerRadius = UDim.new(0, 150)
            UICorner_2.Parent = ToggleParent

            UICorner_3.CornerRadius = UDim.new(0, 3)
            UICorner_3.Parent = Toggle

            ImageLabel.Parent = Toggle
            ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ImageLabel.BackgroundTransparency = 1.000
            ImageLabel.BorderSizePixel = 0
            ImageLabel.Position = UDim2.new(0, 6, 0, 0)
            ImageLabel.Size = UDim2.new(0, 15, 0, 15)
            ImageLabel.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"

            local function toggle()
                on = not on
                f(on)
                ts:Create(Toggle_2,TweenInfo.new(.4),{Position = ((on and onp) or (not on and offp)), BackgroundColor3 = ((on and onc) or (not on and offc))}):Play()
            end

            _1.Activated:Connect(toggle)
            ToggleParent.Activated:Connect(toggle)
            Toggle_2.Activated:Connect(toggle)
        end

        function self2:Textbox(name,f)
            local TextBox = Instance.new("Frame")
            local TextBox_2 = Instance.new("TextBox")
            local TextLabel = Instance.new("TextLabel")
