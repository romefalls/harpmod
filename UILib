            ImageLabel.BackgroundTransparency = 1.000
            ImageLabel.BorderSizePixel = 0
            ImageLabel.Position = UDim2.new(0, 6, 0, 0)
            ImageLabel.Size = UDim2.new(0, 15, 0, 15)
            ImageLabel.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
            local selecting
            local k = default

            game:GetService("UserInputService").InputBegan:Connect(function(m,m2)
                if not m2 then
                    if not selecting then
                        if m.KeyCode == k then
                            f(k)
                        end
                    end
                end
            end)

            local function key()
                _2.Text = "Key: ..."
                selecting = true
                local con; con = game:GetService("UserInputService").InputBegan:Connect(function(m)
                    if m.KeyCode ~= Enum.KeyCode.Unknown then
                        k = m.KeyCode
                        _2.Text = "Key: "..tostring(k):sub(14)
                        selecting = false
                        con:Disconnect()
                    end
                end)
            end

            _2.Activated:Connect(key)
            _1.Activated:Connect(key)
        end

        return self2
    end
    return self
end

